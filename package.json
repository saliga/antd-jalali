{
  "name": "antd-jalali",
  "version": "0.9.4",
  "description": "A wrapper for ant-design datepicker and calendar to support Jalali calendar type with dayjs. (supports CommonJs)",
  "keywords": [
    "ant",
    "datepicker",
    "calendar",
    "jalali",
    "persian",
    "shamsi",
    "dayjs",
    "khorshidi"
  ],
  "author": "SaeedRahimi",
  "homepage": "https://saeedrahimi.github.io/antd-jalali",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/saeedrahimi/antd-jalali"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "typings": "./antd-jalali.d.ts",
  "files": [
    "dist/",
    "antd-jalali.d.ts"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "clean": "rimraf dist/",
    "build:esm": "tsc -p ./tsconfig.json",
    "build:cjs": "tsc -p ./tsconfig-cjs.json"
  },
  "dependencies": {
    "dayjs": "^1.9.6",
    "jalaliday": "^2.3.0",
    "rc-picker": "^2.4.1"
  },
  "peerDependencies": {
    "antd": "^4.8.4",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "antd": "^4.8.4",
    "copyfiles": "^2.4.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "latest",
    "rimraf": "^3.0.2",
    "typescript": "~4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
